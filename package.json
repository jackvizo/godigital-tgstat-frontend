{
  "name": "godigital",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "PORT=$(grep NEXTJS_PORT ./.env | cut -d '=' -f2) next dev",
    "build": "next build",
    "start": "PORT=$(grep NEXTJS_PORT ./.env | cut -d '=' -f2) node .next/standalone/server.js",
    "lint": "next lint",
    "prisma:generate": "prisma generate --schema ./schema.prisma",
    "gql-ts-gen": "gq $(grep HASURA_SCHEMA_URL .env | cut -d '=' -f2) -H \"X-Hasura-Admin-Secret: $(grep HASURA_GRAPHQL_ADMIN_SECRET .env | cut -d '=' -f2)\" -H \"X-Hasura-Role: user\" --introspect > schema.graphql && graphql-codegen --config codegen.ts",
    "gql-ts": "graphql-codegen --config codegen.ts --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.7.2",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.5",
    "@mui/material": "^5.15.5",
    "@mui/material-nextjs": "^5.15.9",
    "@mui/x-date-pickers": "^7.6.2",
    "@prisma/client": "^5.2.0",
    "@tanstack/react-query": "^5.17.19",
    "axios": "^1.6.7",
    "chart.js": "^4.4.3",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-adapter-moment": "^1.0.1",
    "dayjs": "^1.11.11",
    "envalid": "^8.0.0",
    "graphql": "^16.6.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "next": "14.1.1",
    "next-auth": "^4.24.5",
    "next-nprogress-bar": "^2.3.11",
    "prisma": "^5.2.0",
    "qs": "^6.11.2",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18",
    "react-pro-sidebar": "^1.1.0",
    "react-toastify": "^10.0.4",
    "subscriptions-transport-ws": "^0.11.0",
    "telegram": "^2.22.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.0",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.0.7",
    "@graphql-codegen/typescript-operations": "^4.2.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@next/swc-linux-x64-gnu": "14.1.1",
    "@parcel/watcher": "^2.4.1",
    "@tanstack/react-query-devtools": "^5.18.1",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.14.202",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20",
    "@types/qs": "^6.9.11",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.7",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "pg": "^8.12.0",
    "sass": "^1.70.0",
    "typescript": "^5"
